const express = require("express")
const patientModels = require("../models/patientModels")
const routers = express.Router()


routers.post("/add", async (req, res) => {
    let data = req.body
    let patients = new patientModels(data)
    let results = await patients.save()
    res.json({ status: "success" })
})

routers.get("/view",async(req,res)=>{
    let data = await patientModels.find()
    res.json(data)
})

routers.post("/search",async(req,res)=>{
    let input = req.body
    let data = await patientModels.find(input)
    res.json(data)
})


module.exports = routers
